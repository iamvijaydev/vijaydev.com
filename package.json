{
  "name": "vijaydev.com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "css:generate": "esrun ./scripts/cssGenerate/generate.ts",
    "css:bundle": "lightningcss --minify --bundle --targets '>= 0.25%' ./app/styles/styles.css -o ./dist/client/assets/styles.css",
    "build:css": "pnpm css:generate && pnpm css:bundle",
    "mjs:esbuild": "esrun ./scripts/dist.ts",
    "mjs:rollup": "rollup -c",
    "build": "pnpm build:css && pnpm mjs:esbuild && pnpm mjs:rollup",
    "build:prod": "cross-env NODE_ENV=production pnpm build",
    "start": "echo watch:build:css && watch:build:esbuild",
    "preview": "serve dist/client"
  },
  "devDependencies": {
    "@code-hike/mdx": "0.9.0",
    "@floating-ui/dom": "^1.6.8",
    "@floating-ui/react": "^0.26.19",
    "@mdx-js/esbuild": "3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@stefanprobst/rehype-extract-toc": "2.2.0",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.14.2",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/shelljs": "0.8.15",
    "cross-env": "7.0.3",
    "esbuild": "0.21.5",
    "esrun": "3.2.26",
    "fs-extra": "11.2.0",
    "glob": "10.4.2",
    "lightningcss-cli": "1.25.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-slug": "6.0.0",
    "remark-frontmatter": "5.0.0",
    "remark-mdx-frontmatter": "4.0.0",
    "rollup": "4.18.0",
    "serve": "14.2.3",
    "shelljs": "0.8.5",
    "to-vfile": "8.0.0",
    "vfile-matter": "5.0.0",
    "yaml": "2.4.5"
  }
}
